<?xml version="1.0" encoding="UTF-8"?>
<bpel:process xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:ns1="http://www.example.org/test/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" expressionLanguage="urn:active-endpoints:expression-language:xquery1.0" name="test" suppressJoinFailure="yes" targetNamespace="test">
   <bpel:import importType="http://schemas.xmlsoap.org/wsdl/" location="../wsdl/test.wsdl" namespace="http://www.example.org/test/"/>
   <bpel:partnerLinks>
      <bpel:partnerLink myRole="proces" name="testPartnerLinkType" partnerLinkType="ns1:testPartnerLinkType"/>
   </bpel:partnerLinks>
   <bpel:variables>
      <bpel:variable element="ns1:NewOperation" name="NewOperation"/>
      <bpel:variable element="ns1:NewOperationResponse" name="NewOperationResponse"/>
   </bpel:variables>
   <bpel:sequence>
      <bpel:receive createInstance="yes" operation="NewOperation" partnerLink="testPartnerLinkType" variable="NewOperation"/>
      <bpel:assign>
         <bpel:copy>
            <bpel:from>
               <bpel:literal>
                  <ns1:NewOperationResponse>
   <out>SUCCESS</out>
</ns1:NewOperationResponse>
               </bpel:literal>
            </bpel:from>
            <bpel:to variable="NewOperationResponse"/>
         </bpel:copy>
      </bpel:assign>
      <bpel:reply operation="NewOperation" partnerLink="testPartnerLinkType" variable="NewOperationResponse"/>
   </bpel:sequence>
</bpel:process>
