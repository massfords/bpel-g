<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03"
        xmlns:ex="http://www.example.org/betsy/" replace.existing="true">

    <process name="proc:start-error" xmlns:proc="http://www.example.org/">
        <provide partnerLink="plt">
            <service name="ex:betsyStartErrorService" port="betsyStartError"/>
        </provide>
    </process>

    <process name="proc:dupe-variable" xmlns:proc="http://www.example.org/">
        <provide partnerLink="plt">
            <service name="ex:betsyDupeVarService" port="betsyDupeVar"/>
        </provide>
    </process>

    <process name="proc:pick-onAlarm-for" xmlns:proc="http://www.example.org/">
        <provide partnerLink="plt">
            <service name="ex:betsyQuoteService" port="betsyQuoteService"/>
        </provide>
    </process>
    <process name="proc:pick-onAlarm-until" xmlns:proc="http://www.example.org/">
        <provide partnerLink="plt">
            <service name="ex:betsyQuoteUntilService" port="betsyQuoteUntilService"/>
        </provide>
    </process>

    <process name="proc:wait" xmlns:proc="http://www.example.org/">
        <provide partnerLink="plt">
            <service name="ex:betsyWaitService" port="betsyWaitService"/>
        </provide>
    </process>

    <process name="proc:event-onAlarm-for" xmlns:proc="http://www.example.org/">
        <provide partnerLink="plt">
            <service name="ex:betsyEventService" port="betsyEventService"/>
        </provide>
    </process>

    <process name="proc:event-onAlarm-until" xmlns:proc="http://www.example.org/">
        <provide partnerLink="plt">
            <service name="ex:betsyEventUntilService" port="betsyEventUntilService"/>
        </provide>
    </process>
</deploy>
