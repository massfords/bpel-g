<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>bpel-g</groupId>
  <artifactId>bpel-g-web</artifactId>
  <version>0.8-SNAPSHOT</version>
  <packaging>war</packaging>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.6</source>
          <target>1.6</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies>
  	<dependency>
  		<groupId>org.activebpel</groupId>
  		<artifactId>ae-war</artifactId>
  		<version>5.0.2</version>
  	</dependency>
    <dependency>
        <groupId>org.activebpel</groupId>
        <artifactId>ae-axisbpelweb</artifactId>
        <version>5.0.2</version>
    </dependency>
    <dependency>
        <groupId>org.activebpel</groupId>
        <artifactId>ae-bpelwar</artifactId>
        <version>5.0.2</version>
    </dependency>
    <dependency>
    	<groupId>org.activebpel</groupId>
    	<artifactId>ae-bpelexpr</artifactId>
    	<version>5.0.2</version>
    </dependency>
    <dependency>
    	<groupId>org.activebpel</groupId>
    	<artifactId>ae-bpelexprbsf</artifactId>
    	<version>5.0.2</version>
    </dependency>
    <dependency>
    	<groupId>org.activebpel</groupId>
    	<artifactId>ae-http</artifactId>
    	<version>5.0.2</version>
    </dependency>
  </dependencies>
</project>
