   </wsdl:message>
   <wsdl:message name="getProcessListResponse">
      <wsdl:part name="output" element="tns:getProcessListOutput"/>
   </wsdl:message>

   <wsdl:message name="getProcessDetailRequest">
      <wsdl:part name="input" element="tns:getProcessDetailInput"/>
   </wsdl:message>
   <wsdl:message name="getProcessDetailResponse">
      <wsdl:part name="output" element="tns:getProcessDetailOutput"/>
   </wsdl:message>

   <wsdl:message name="getAPIVersionRequest">
