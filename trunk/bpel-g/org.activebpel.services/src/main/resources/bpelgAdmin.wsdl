   </wsdl:message>
   <wsdl:message name="getProcessDefResponse">
      <wsdl:part name="output" element="tns:getProcessDefOutput"/>
   </wsdl:message>

   <wsdl:message name="getProcessLogRequest">
      <wsdl:part name="input" element="tns:getProcessLogInput"/>
   </wsdl:message>
   <wsdl:message name="getProcessLogResponse">
      <wsdl:part name="output" element="tns:getProcessLogOutput"/>
   </wsdl:message>

   <wsdl:message name="getVariableDataRequest">
      <wsdl:part name="input" element="tns:getVariableDataInput"/>
   </wsdl:message>
   <wsdl:message name="getVariableDataResponse">
      <wsdl:part name="output" element="tns:getVariableDataOutput"/>
   </wsdl:message>

   <wsdl:message name="setVariableDataRequest">
